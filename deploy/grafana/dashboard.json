{
  "dashboard": {
    "title": "zen-lead Controller Dashboard",
    "tags": ["zen-lead", "kubernetes", "leader-election"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Leader Services Total",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(zen_lead_leader_services_total)",
            "legendFormat": "Total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Pods Available",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(zen_lead_pods_available)",
            "legendFormat": "Total Available",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 0, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Failover Count (Total)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(zen_lead_failover_count_total)",
            "legendFormat": "Total Failovers",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Failover Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(zen_lead_failover_count_total[5m]))",
            "legendFormat": "Failovers/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.1, "color": "red"}
              ]
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 5,
        "title": "Leader Duration by Service",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "zen_lead_leader_duration_seconds",
            "legendFormat": "{{namespace}}/{{service}} - {{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 6,
        "title": "Pods Available by Service",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "zen_lead_pods_available",
            "legendFormat": "{{namespace}}/{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Pods"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 7,
        "title": "Reconciliation Duration (P95, P50)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(zen_lead_reconciliation_duration_seconds_bucket{result=\"success\"}[5m])) by (le, namespace, service))",
            "legendFormat": "P95 - {{namespace}}/{{service}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(zen_lead_reconciliation_duration_seconds_bucket{result=\"success\"}[5m])) by (le, namespace, service))",
            "legendFormat": "P50 - {{namespace}}/{{service}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 8,
        "title": "Reconciliation Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(zen_lead_reconciliations_total[5m])) by (namespace, service, result)",
            "legendFormat": "{{namespace}}/{{service}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Reconciliations/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 9,
        "title": "Failover Rate Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(zen_lead_failover_count_total[5m])",
            "legendFormat": "{{namespace}}/{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Failovers/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 10,
        "title": "Sticky Leader Hit/Miss Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(zen_lead_sticky_leader_hits_total[5m])",
            "legendFormat": "Hits - {{namespace}}/{{service}}",
            "refId": "A"
          },
          {
            "expr": "rate(zen_lead_sticky_leader_misses_total[5m])",
            "legendFormat": "Misses - {{namespace}}/{{service}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Rate"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 11,
        "title": "Reconciliation Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(zen_lead_reconciliation_errors_total[5m])",
            "legendFormat": "{{namespace}}/{{service}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Errors/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 12,
        "title": "Port Resolution Failures",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "rate(zen_lead_port_resolution_failures_total[5m])",
            "legendFormat": "{{namespace}}/{{service}} - {{port_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Failures/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 13,
        "title": "Leader Pod Age",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "zen_lead_leader_pod_age_seconds",
            "legendFormat": "{{namespace}}/{{service}} - {{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Age (seconds)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 14,
        "title": "Leader Services Without Endpoints",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "zen_lead_leader_service_without_endpoints",
            "legendFormat": "{{namespace}}/{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Has No Endpoints (1=yes, 0=no)"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 15,
        "title": "Leader Selection Attempts",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "rate(zen_lead_leader_selection_attempts_total[5m])",
            "legendFormat": "{{namespace}}/{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Attempts/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 16,
        "title": "Leader Services and EndpointSlices by Namespace",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44},
        "targets": [
          {
            "expr": "zen_lead_leader_services_total",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "zen_lead_endpointslices_total",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ]
      },
      {
        "id": 17,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 52},
        "targets": [
          {
            "expr": "zen_lead_cache_size",
            "legendFormat": "Cache Size - {{namespace}}",
            "refId": "A"
          },
          {
            "expr": "rate(zen_lead_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec - {{namespace}}",
            "refId": "B"
          },
          {
            "expr": "rate(zen_lead_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec - {{namespace}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Count"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 18,
        "title": "Cache Hit Ratio",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 52},
        "targets": [
          {
            "expr": "sum(rate(zen_lead_cache_hits_total[5m])) / (sum(rate(zen_lead_cache_hits_total[5m])) + sum(rate(zen_lead_cache_misses_total[5m])))",
            "legendFormat": "Hit Ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.8, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 19,
        "title": "Cache Update Duration",
        "type": "graph",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 52},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(zen_lead_cache_update_duration_seconds_bucket[5m])) by (le, namespace))",
            "legendFormat": "P95 - {{namespace}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 20,
        "title": "Retry Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 60},
        "targets": [
          {
            "expr": "rate(zen_lead_retry_attempts_total[5m])",
            "legendFormat": "Retry Attempts/sec - {{namespace}}/{{service}}/{{operation}}/{{attempt}}",
            "refId": "A"
          },
          {
            "expr": "rate(zen_lead_retry_success_after_retry_total[5m])",
            "legendFormat": "Success After Retry/sec - {{namespace}}/{{service}}/{{operation}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Rate"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 21,
        "title": "API Call Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 60},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(zen_lead_api_call_duration_seconds_bucket{result=\"success\"}[5m])) by (le, namespace, service, operation))",
            "legendFormat": "P95 - {{namespace}}/{{service}}/{{operation}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(zen_lead_api_call_duration_seconds_bucket{result=\"success\"}[5m])) by (le, namespace, service, operation))",
            "legendFormat": "P50 - {{namespace}}/{{service}}/{{operation}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 22,
        "title": "Failover Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 68},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(zen_lead_failover_latency_seconds_bucket[5m])) by (le, namespace, service, reason))",
            "legendFormat": "P95 - {{namespace}}/{{service}}/{{reason}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(zen_lead_failover_latency_seconds_bucket[5m])) by (le, namespace, service, reason))",
            "legendFormat": "P50 - {{namespace}}/{{service}}/{{reason}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      },
      {
        "id": 23,
        "title": "Timeout Occurrences",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 68},
        "targets": [
          {
            "expr": "rate(zen_lead_timeout_occurrences_total[5m])",
            "legendFormat": "{{namespace}}/{{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Timeouts/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

